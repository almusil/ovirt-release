[ovirt-@OVIRT_SLOT@-epel]
name=Extra Packages for Enterprise Linux 7 - $basearch
#baseurl=http://download.fedoraproject.org/pub/epel/7/$basearch
mirrorlist=https://mirrors.fedoraproject.org/metalink?repo=epel-7&arch=$basearch
failovermethod=priority
enabled=1
includepkgs=
 ansible,
 ansible-doc,
 epel-release,
 facter,
 golang,
 golang-github-stretchr-testify-devel,
 golang-bin,
 golang-github-davecgh-go-spew-devel,
 golang-github-pmezard-go-difflib-devel,
 golang-github-stretchr-objx-devel,
 golang-src,
 hiera,
 libtomcrypt,
 libtommath,
 ovirt-guest-agent*,
 puppet,
 python2-crypto,
 python2-ecdsa,
 python-ordereddict,
 ruby-augeas,
 rubygem-rgen,
 ruby-shadow

gpgcheck=1
gpgkey=https://dl.fedoraproject.org/pub/epel/RPM-GPG-KEY-EPEL-7

[ovirt-@OVIRT_SLOT@-centos-gluster5]
name=CentOS-$releasever - Gluster 5
baseurl=http://mirror.centos.org/altarch/7/storage/$basearch/gluster-5/
gpgcheck=1
enabled=1
gpgkey=https://www.centos.org/keys/RPM-GPG-KEY-CentOS-SIG-Storage

[ovirt-@OVIRT_SLOT@-virtio-win-latest]
name=virtio-win builds roughly matching what will be shipped in upcoming RHEL
baseurl=http://fedorapeople.org/groups/virt/virtio-win/repo/latest
enabled=1
gpgcheck=0

[ovirt-@OVIRT_SLOT@-centos-qemu-ev]
name=CentOS-7 - QEMU EV
baseurl=http://mirror.centos.org/altarch/7/virt/$basearch/kvm-common/
gpgcheck=1
enabled=1
gpgkey=https://www.centos.org/keys/RPM-GPG-KEY-CentOS-SIG-Virtualization

[ovirt-@OVIRT_SLOT@-centos-ovirt43]
name=CentOS-7 - oVirt 4.3
baseurl=http://mirror.centos.org/altarch/7/virt/$basearch/ovirt-4.3/
gpgcheck=1
enabled=1
gpgkey=https://www.centos.org/keys/RPM-GPG-KEY-CentOS-SIG-Virtualization

[ovirt-@OVIRT_SLOT@-centos-opstools]
name=CentOS-7 - OpsTools - release
baseurl=http://mirror.centos.org/altarch/7/opstools/$basearch/
gpgcheck=1
enabled=1
gpgkey=https://www.centos.org/keys/RPM-GPG-KEY-CentOS-SIG-OpsTools

# This repo provides only x86-64 repos but all the packages are noarch.
[sac-gluster-ansible]
name=Copr repo for gluster-ansible owned by sac
baseurl=https://copr-be.cloud.fedoraproject.org/results/sac/gluster-ansible/epel-7-x86_64/
type=rpm-md
skip_if_unavailable=False
gpgcheck=1
gpgkey=https://copr-be.cloud.fedoraproject.org/results/sac/gluster-ansible/pubkey.gpg
repo_gpgcheck=0
enabled=1
enabled_metadata=1
