[ovirt-@OVIRT_SLOT@-epel]
name=Extra Packages for Enterprise Linux 7 - $basearch
#baseurl=http://download.fedoraproject.org/pub/epel/7/$basearch
mirrorlist=https://mirrors.fedoraproject.org/metalink?repo=epel-7&arch=$basearch
failovermethod=priority
enabled=1
includepkgs=
 epel-release,
 ioprocess,
 libtomcrypt,
 libtommath,
 ovirt-guest-agent*,
 puppet,
 python2-crypto,
 python2-ecdsa,
 python34*,
 python-ecdsa,
 python-ioprocess,
 python-ordereddict,
 python-ply,
 rubygem-rgen

gpgcheck=1
gpgkey=https://dl.fedoraproject.org/pub/epel/RPM-GPG-KEY-EPEL-7

[ovirt-@OVIRT_SLOT@-centos-gluster312]
name=CentOS-7 - Gluster 3.12
baseurl=http://mirror.centos.org/altarch/7/storage/$basearch/gluster-3.12/
gpgcheck=1
gpgkey=https://raw.githubusercontent.com/CentOS-Storage-SIG/centos-release-storage-common/master/RPM-GPG-KEY-CentOS-SIG-Storage
enabled=1

[ovirt-@OVIRT_SLOT@-virtio-win-latest]
name=virtio-win builds roughly matching what will be shipped in upcoming RHEL
baseurl=http://fedorapeople.org/groups/virt/virtio-win/repo/latest
enabled=1
skip_if_unavailable=1
gpgcheck=0

[ovirt-@OVIRT_SLOT@-centos-qemu-ev]
name=CentOS-7 - QEMU EV
baseurl=http://mirror.centos.org/altarch/7/virt/$basearch/kvm-common/
gpgcheck=1
enabled=1
gpgkey=https://www.centos.org/keys/RPM-GPG-KEY-CentOS-SIG-Virtualization

[ovirt-@OVIRT_SLOT@-centos-ovirt42]
name=CentOS-7 - oVirt 4.2
baseurl=http://mirror.centos.org/altarch/7/virt/$basearch/ovirt-4.2/
gpgcheck=1
enabled=1
gpgkey=https://www.centos.org/keys/RPM-GPG-KEY-CentOS-SIG-Virtualization

[opstools7-fluentd-012-release]
name=CentOS-7 - OpsTools - fluentd release repo
baseurl=https://cbs.centos.org/repos/opstools7-fluentd-012-release/$basearch/os
gpgcheck=0
enabled=1

[opstools7-perfmon-common-release]
name=CentOS-7 - OpsTools - perfmon release repo
baseurl=https://cbs.centos.org/repos/opstools7-perfmon-common-release/$basearch/os
gpgcheck=0
enabled=1

[opstools7-common-release]
name=CentOS-7 - OpsTools - common release repo
baseurl=https://cbs.centos.org/repos/opstools7-common-release/$basearch/os
gpgcheck=0
enabled=1
